import java.util.*;
class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int n=sc.nextInt();
        
        for(int i=1;i<=n;i++){
            int val=i;
            for(int j=1; j<=i; j++){
                System.out.print(j +" ");
            }
            for(int k=1; k<=i-1; k++){
                System.out.print(k +" ");
            }
            System.out.println();
        }
    }
}
