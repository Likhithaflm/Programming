import java.util.*;
class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int n=sc.nextInt();
        
        for(int i=1;i<=n;i++){
            int val=1;
            for(int j=1; j<=2*i-1; j++){
                if(j<i){
                    System.out.print(val++ +" ");
                }
                else{
                    System.out.print(val-- +" ");
                }
                
            }
            System.out.println();
        }
    }
}

