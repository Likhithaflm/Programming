Code:

import java.util.*;
class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int n=sc.nextInt();
        
        for(int i=1;i<=n;i++){
            int val=i;
            for(int j=1; j<=(n-i)+1;j++){
                System.out.print(val++ + " ");
            }
            System.out.println();
        }
    }
}

